birth_dict={'Albert Einstein':'03/14/1879',
            'Benjamin Franklin':'01/17/1706',
            'Ada Lovelace':'12/10/1815'}

print('Welcome to the birthday dictionary. We know the birthdays of:')
for i in birth_dict.keys():
    print(i)
q=input(r"Who's birthday do you want to look up? ")
if q in birth_dict:
    print('{}\'s birthday is {}.'.format(q,str(birth_dict[q])))
else:
    print('we don\'t have {}\'s birthday.'.format(q))
